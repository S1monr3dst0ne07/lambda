#lib/base.lam
#lib/nat.lam
#lib/bool.lam

-- arith first order
ADD     = (\a\b b SUCC a)
SUB     = (\a\b b PRED a)

-- comparison
IS-POS       = (\n n (CONST TRUE) FALSE)
IS-GREATER   = (\a\b IS-POS (SUB a b))

-- arith second order
MUL     = (\a\b a (ADD b) ZERO)

-- reification
PRINT   = (\x x ? ?)
