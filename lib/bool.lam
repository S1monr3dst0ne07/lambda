-- church encoded booleans
BOOL::TRUE    = (\a\b a)
BOOL::FALSE   = (\a\b b)

-- boolean operators
BOOL::NOT     = (\m m BOOL::FALSE BOOL::TRUE)
BOOL::IOR     = (\m\n m BOOL::TRUE n)
BOOL::AND     = (\m\n m n BOOL::FALSE)
BOOL::XOR     = (\m\n BOOL::AND (BOOL::IOR n m) (BOOL::NOT (BOOL::AND n m)))

